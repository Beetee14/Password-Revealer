{
    "manifest_version": 2,
    "name": "Password Revealer",
    "version": "0.1",
    "permissions": [
    "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.1.1.min.js", "content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    }

}
